<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PerfBundle\Entity\Offices" table="offices">
    <id name="officecode" type="string" column="officeCode" length="10">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="city" type="string" column="city" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addressline1" type="string" column="addressLine1" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addressline2" type="string" column="addressLine2" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="state" type="string" column="state" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="postalcode" type="string" column="postalCode" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="territory" type="string" column="territory" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
